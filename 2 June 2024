Q-Link : https://leetcode.com/problems/reverse-string/submissions/1275507550/?envType=daily-question&envId=2024-06-02

class Solution {
public:
    void reverseString(vector<char>& s) {
        int l=0;
        int h=s.size()-1;
        while(l<h){
            swap(s[l],s[h]);
            l++;
            h--;
        }
    }
};
